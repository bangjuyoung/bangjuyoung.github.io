    "next": "15.5.0",
    "next-intl": "^4.3.4",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "rimraf": "^6.0.1",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
